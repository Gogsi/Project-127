<Window x:Class="Project_127.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_127"
        mc:Ignorable="d"
        Title="Settings" Height="171.22" Width="213.927">
    <Grid>
        <Button x:Name="InstallationPathButton" Content="GTA V installation path" HorizontalAlignment="Left" VerticalAlignment="Top" Width="138" Margin="0,20,0,0" Click="InstallationPathButton_Click"/>
        <Button x:Name="LauncherPathButton" Content="Launcher files path" HorizontalAlignment="Left" Margin="0,40,0,0" VerticalAlignment="Top" Width="138" Click="LauncherPathButton_Click"/>
        <CheckBox x:Name="_500KCheckBox" Content="500K Bonus" HorizontalAlignment="Left" Margin="0,60,0,0" VerticalAlignment="Top" Click="_500KCheckBox_Click"/>
        <Button x:Name="ResetButton" Content="Reset settings to default" HorizontalAlignment="Left" Margin="0,75,0,0" VerticalAlignment="Top" Width="138" Click="ResetButton_Click"/>
    </Grid>
</Window>
